<?xml version="1.0" encoding="UTF-8"?>
<!-- config.xml -->
<system>
    <maxreq>1000</maxreq>
    <maxproc>2</maxproc>
    <downloader>
        <dblocksize>102400</dblocksize>
        <httptimeout>30000</httptimeout>
    </downloader>
    <uploader>
        <ublocksize>102400</ublocksize>
    </uploader>
   <cloud>
		<connection>
		</connection>
		<processqueue>videoqueue</processqueue>
		<cancelqueue>cancelqueue</cancelqueue>
		<container>input</container>
	</cloud>
    <callback>

    </callback>
    <transcoconfigs>
        <tconfig name="default">
            <workdir>
                <input>/home/marc/input</input>
                <output>/home/marc/output</output>
            </workdir>
            <timeout>86400</timeout>
            <live>false</live>
            <profiles>
                <profile name="profile1"/>
                <profile name="profile2"/>
                <profile name="profile4"/>
            </profiles>
        </tconfig>
        <tconfig name="live">
            <workdir>
                <input>/home/marc/input</input>
                <output>/home/marc/output</output>
            </workdir>
            <timeout>86400</timeout>
            <live>true</live>
            <profiles>
                <profile name="profile3"/>
            </profiles>
        </tconfig>
    </transcoconfigs>
</system>