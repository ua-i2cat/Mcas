<?xml version="1.0" encoding="UTF-8"?>
<!-- profiles.xml -->
<profiles>
    <profile name="H264">
        <format>mp4</format>
        <vcodec>libx264</vcodec>
        <acodec>libfdk_aac</acodec>
        <additionalFlags></additionalFlags>
    </profile>
    <profile name="WebM">
        <format>webm</format>
        <vcodec>libvpx</vcodec>
        <acodec>libvorbis</acodec>
        <additionalFlags> </additionalFlags>
    </profile>
    <profile name="HLS" class="HLS">
        <format>m3u8</format>
        <vcodec>libx264</vcodec>
        <acodec>libfaac</acodec>
        <additionalFlags>-profile:v baseline -map 0 -flags -global_header</additionalFlags>
        <segDuration>3</segDuration>
        <windowLength>3</windowLength>
    </profile>
    <profile name="Dash" class="DASH">
    	<format>mpd</format>
        <vcodec>libx264</vcodec>
        <acodec></acodec>
        <additionalFlags>-profile:v baseline -map 0 -flags -global_header</additionalFlags>
        <segDuration>1</segDuration>
        <windowLength>10</windowLength>
    </profile>
    <profile name="RTMP" class="RTMP">
        <format>flv</format>
        <vcodec>libx264</vcodec>
        <acodec>libmp3lame</acodec>
        <additionalFlags>-ar 44100</additionalFlags>
        <domain>192.168.10.49</domain>
        <application>live</application>
    </profile> 
</profiles>
